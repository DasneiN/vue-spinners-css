{
  "name": "vue-spinners-css",
  "version": "0.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/JoshK2/vue-spinners-css"
  },
  "bugs": "https://github.com/JoshK2/vue-spinners-css/issues",
  "homepage": "https://github.com/JoshK2/vue-spinners-css",
  "keywords": [
    "vue-spinners",
    "vue-spinner",
    "loadingio",
    "loading-animations",
    "loaders",
    "css-animations",
    "vuejs",
    "component",
    "vue-component",
    "vue",
    "spinner",
    "spinners",
    "loading",
    "loader",
    "progress",
    "activity"
  ],
  "author": "JoshK2",
  "license": "MIT",
  "main": "dist/vue-spinners-css.common.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --name vue-spinners-css ./src/lib.js",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"src/**/*.vue\" \"src/**/*.js\""
  },
  "peerDependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bit": {
    "env": {
      "compiler": "bit.envs/bundlers/vue@2.5.1"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
